{
  "ReverseProxy": {
    "Routes": {
      "orders-route": {
        "ClusterId": "orders-cluster",
        "Match": {
          "Path": "/orders/{**catch-all}"
        }
      },
      "inventory-route": {
        "ClusterId": "inventory-cluster",
        "Match": {
          "Path": "/inventory/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "orders-cluster": {
        "Destinations": {
          "orders-1": {
            "Address": "http://orderservice:8080"
          }
        }
      },
      "inventory-cluster": {
        "Destinations": {
          "inventory-1": {
            "Address": "http://inventoryservice:8080"
          }
        }
      }
    }
  }
}